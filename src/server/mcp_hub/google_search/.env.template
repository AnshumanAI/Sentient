# server/mcp_hub/google_search/.env.template

# Google Custom Search API Configuration
# 1. Get an API Key from your Google Cloud project: https://console.cloud.google.com/apis/credentials
# 2. Create a Programmable Search Engine and get its ID (CX): https://programmablesearchengine.google.com/
#    - In the setup for the search engine, make sure to enable "Search the entire web".
GOOGLE_API_KEY=your_google_api_key
GOOGLE_CSE_ID=your_search_engine_id_cx

# HTTP Server configuration for FastMCP
MCP_SERVER_HOST=127.0.0.1
MCP_SERVER_PORT=9005